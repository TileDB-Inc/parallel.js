{
  "name": "paralleljs",
  "version": "1.1.0",
  "description": "parallel.js enables easy multi-thread processing in javascript",
  "author": "Adam Savitzky <adam.savitzky@gmail.com>",
  "contributors": [
    "Sebastian Mayr <sebmaster16@gmail.com> (http://s3bmaster.blogspot.co.at/)",
    "Amila Welihinda <amilajack@gmail.com> (https://amilajack.com/)"
  ],
  "license": "MIT",
  "main": "./lib/paralleljs.js",
  "types": "./lib/index.d.ts",
  "type": "module",
  "scripts": {
    "lint": "eslint lib",
    "test": "vitest",
    "build": "vite build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/parallel-js/parallel.js.git"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "keywords": [
    "parallel",
    "spawn",
    "map",
    "thread",
    "parallel.js",
    "workers",
    "webworkers"
  ],
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.3.1",
    "eslint": "^9.35.0",
    "is-ci": "^4.1.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "7.1.5",
    "vite-plugin-dts": "4.5.4",
    "vitest": "3.2.4"
  },
  "browser": {
    "child_process": false
  },
  "engines": {
    "node": ">=v18.18.0"
  },
  "testling": {
    "scripts": [
      "lib/parallel.js",
      "test/jasmine/jasmine.js",
      "test/jasmine/jasmine.tap_reporter.js",
      "test/specs/*.js",
      "test/runner.js"
    ],
    "browsers": [
      "ie/9..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "chrome": "58",
            "node": "10"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "bliss",
    "env": {
      "jest": false,
      "jasmine": true,
      "browser": true
    }
  },
  "renovate": {
    "extends": [
      "bliss"
    ]
  }
}
